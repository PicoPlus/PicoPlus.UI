{
  "app": "ipicoplus",
  "platform": "dotnet",
  "runtime": "dotnet-8",
  "port": 5000,
  "build": {
    "location": "iran"
  },
  "disks": [
    {
      "name": "picoapp",
      "mountTo": "/app/data"
    }
